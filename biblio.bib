issn = {0163-1829},
journal = {Physical Review B},
month = oct,
number = {10},
pages = {7196--7199},
publisher = {APS},
title = {{Symmetry of the 3d\^{}\{9\} ligand hole induced by doping in YBa\_\{2\}Cu\_\{3\}O\_\{7-$\delta$\}}},
url = {http://prb.aps.org/abstract/PRB/v38/i10/p7196\_1 http://link.aps.org/doi/10.1103/PhysRevB.38.7196},
volume = {38},
year = {1988}
}
@article{Bussmann-Holder2003,
abstract = {The model of two (s and p) channel superconductivity known to be necessary to explain the superconductivity of MgB2 has been applied to the Al1-xMgxB2 diborides by tuning x from MgB2 to AlMgB4. The interband coupling paramenter (probing the strength of the interchannel pairing due to quantum interferences effects) and the two gaps in the s and p channel have been calculated as a function of x by using experimental data for the two phonon mode energies which are most relevant for the pairing interaction. It is found that the two gaps reverse in leading order around x=0.6 and that in comparison with the dominant intra-s-band single channel pairing an increase in Tc of 1.5 occurs for MgB2 while in AlMgB4 the dominant p band sigle channel pairing leads to enhancement of Tc of 100.},
