file = {:home/andres/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Starowicz et al. - 2013 - A flat band at the chemical potential of a Fe1.03Te0.94S0.06 superconductor observed by angle-resolved photoem.pdf:pdf},
issn = {1361-648X},
journal = {Journal of Physics: Condensed Matter},
month = may,
number = {19},
pages = {195701},
pmid = {23604265},
title = {{A flat band at the chemical potential of a Fe1.03Te0.94S0.06 superconductor observed by angle-resolved photoemission spectroscopy.}},
url = {http://iopscience.iop.org/0953-8984/25/19/195701/},
volume = {25},
year = {2013}
}
@article{Pomjakushin2011a,
