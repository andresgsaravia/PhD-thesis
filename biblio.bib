pages = {65011},
title = {{Growth conditions, structure and superconductivity of pure and metal-doped FeTe 1 − x Se x single crystals}},
url = {http://stacks.iop.org/0953-2048/24/i=6/a=065011},
volume = {24},
year = {2011}
