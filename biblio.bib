doi = {10.1103/PhysRevB.83.174522},
file = {:home/andres/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Seibold, Grilli, Lorenzana - 2011 - Influence of correlations on transitive electron-phonon couplings in cuprate superconductors.pdf:pdf},
