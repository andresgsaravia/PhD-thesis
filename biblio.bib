journal = {Nature materials},
month = oct,
number = {10},
pages = {733--6},
pmid = {21857676},
publisher = {Nature Publishing Group},
title = {{Evolution and control of oxygen order in a cuprate superconductor.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21857676},
volume = {10},
year = {2011}
}
